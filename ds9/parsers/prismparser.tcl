                    8 { PrismCmdLoad $1 }
                    9 { PrismCmdLoad $2 }
                    10 { ProcessCmdCVAR0 PrismDestroy }
                    11 { ProcessCmdCVAR0 PrismClear }
                    13 { ProcessCmdCVAR0 PrismImage }
                    16 { PrismCmdRef $2 }
