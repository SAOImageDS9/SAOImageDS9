                    19 { ProcessCmdCVAR3 col $1 num $2 plot,mode $3 PrismHistogramGenerate }
                    20 { ProcessCmdCVAR6 xx $1 yy $2 xerr {} yerr {} plot,type $3 plot,mode $4 PrismPlotGenerate }
