if { $argc < 1 } {
    puts "Usage: $argv0 ImageFormat"
    exit 1
}

set fmt [lindex $argv 0]  
set argv [lrange $argv 1 end]
set testFile "$fmt.test"
if { ![file exists $testFile] } {
    puts "Test file $testFile not existent."
    exit 0
}

puts "Running test $testFile"
source $testFile

exit 0
